        "id": 268885,
        "name": "",
        "username": "Herohtar",
        "avatar_template": "/user_avatar/community.signalusers.org/herohtar/{size}/43780_2.png",
        "created_at": "2025-05-29T04:31:26.364Z",
        "cooked": "<p>Signal on Android stores images in its encrypted database. If you would like to make them accessible externally you will need to manually save (‚Äúdownload‚Äù) the images to your device from within Signal.</p>",
        "post_number": 2,
        "post_type": 1,
        "posts_count": 5,
        "updated_at": "2025-05-29T04:31:45.969Z",
        "reply_count": 1,
        "reply_to_post_number": null,
        "quote_count": 0,
